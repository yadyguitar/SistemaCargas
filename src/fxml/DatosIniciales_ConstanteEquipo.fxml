<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>

<TitledPane  text="Constantes del equipo" xmlns:fx="http://javafx.com/fxml" fx:controller="controllers.ConstanteEquipo">
			<content>
   				<AnchorPane >
       				<GridPane fx:id="constantes" style="-fx-font-size: 14"  gridLinesVisible="true" AnchorPane.topAnchor="10" AnchorPane.leftAnchor="20" AnchorPane.bottomAnchor="10">
           				<Label minWidth="150" text=" Diámetro (cm)" GridPane.columnIndex="0" GridPane.rowIndex="0" ></Label>
           				<TextField fx:id="diametroFXML" GridPane.columnIndex="1" GridPane.rowIndex="0" ></TextField>
           				<Label text=" Altura (cm)" GridPane.columnIndex="0" GridPane.rowIndex="1" ></Label>
           				<TextField fx:id="alturaFXML"  GridPane.columnIndex="1" GridPane.rowIndex="1" ></TextField>
           				<Label   text=" Área (cm²)" GridPane.columnIndex="0" GridPane.rowIndex="2" ></Label>
           				<TextField  fx:id="areaFXML"  style="-fx-background-color: transparent; -fx-cursor: DEFAULT ;"  editable="false" GridPane.columnIndex="1" GridPane.rowIndex="2" ></TextField>
           				<Label text=" Volumen (cm³)" GridPane.columnIndex="0" GridPane.rowIndex="3" ></Label>
           				<TextField fx:id="volumenFXML"  style="-fx-background-color: transparent; -fx-cursor: DEFAULT ;"  editable="false" GridPane.columnIndex="1" GridPane.rowIndex="3" ></TextField>
           				<Label text=" Peso Anillo (grs)" GridPane.columnIndex="0" GridPane.rowIndex="4" ></Label>
           				<TextField fx:id="pesoAnilloFXML"  GridPane.columnIndex="1" GridPane.rowIndex="4" ></TextField>
           				<Label text=" Cte. Aparato " GridPane.columnIndex="0" GridPane.rowIndex="5" ></Label>
           				<TextField fx:id="cteAparatoFXML"  GridPane.columnIndex="1" GridPane.rowIndex="5" ></TextField>
	          		</GridPane>
   				</AnchorPane>
			</content>
		 </TitledPane>